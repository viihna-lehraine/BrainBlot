[req]
default_bits		= 2048
prompt			= no
default_md		= sha256
distinguished_name	= req_distinguished_name
req_extensions		= req_ext

[req_distinguished_name]
C	= US
ST	= State
L	= Locality
O	= Your company
OU	= Your division
CN	= localhost

[req_ext]
subjectAltName	= @alt_names

[alt_names]
DNS.1	= localhost
IP.1	= 127.0.0.1
