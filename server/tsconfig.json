{
	"compilerOptions": {
		"allowSyntheticDefaultImports": true,
		"allowUnreachableCode": false,
		"allowUnusedLabels": true,
		"alwaysStrict": true,
		"baseUrl": ".",
		"composite": true,
		"declaration": true,
		"declarationDir": "backend/types/generated",
		"declarationMap": true,
		"disableSolutionSearching": false,
		"disableReferencedProjectLoad": true,
		"esModuleInterop": true,
		"experimentalDecorators": true,
		"exactOptionalPropertyTypes": true,
		"forceConsistentCasingInFileNames": true,
		"importHelpers": true,
		"incremental": true,
		"inlineSourceMap": true,
		"inlineSources": true,
		"isolatedModules": true,
		"lib": ["ES2022"],
		"module": "ES2022",
		"moduleDetection": "auto",
		"moduleResolution": "node",
		"newLine": "lf",
		"noEmit": false,
		"noEmitOnError": false,
		"noFallthroughCasesInSwitch": true,
		"noImplicitAny": true,
		"noImplicitOverride": true,
		"noImplicitReturns": true,
		"noImplicitThis": true,
		"noPropertyAccessFromIndexSignature": false,
		"noResolve": false,
		"noUnusedLocals": false,
		"noUnusedParameters": false,
		"outDir": "./dist/",
		"paths": {
			"@types/*": ["./backend/types/*"]
		},
		"preserveSymlinks": true,
		"removeComments": false,
		"resolveJsonModule": true,
		"rootDir": "./backend/src/",
		"skipLibCheck": true,
		"strict": true,
		"strictBindCallApply": true,
		"strictFunctionTypes": true,
		"strictNullChecks": true,
		"strictPropertyInitialization": true,
		"target": "ES2022",
		"tsBuildInfoFile": "./dist/tsconfig.tsbuildinfo",
		"typeRoots": ["./node_modules/@types", "backend/types"],
		"types": ["node"],
		"useDefineForClassFields": true,
		"useUnknownInCatchVariables": true
	},
	"include": ["./backend/src/**/*.ts", "./backend/types/**/*.d.ts", "../.dev/viihna/backend/oldfiles/review-necessity/admin.ts", "../.dev/viihna/backend/oldfiles/review-necessity/Vault.ts"],
	"exclude": ["./node_modules", "./dist/", "**/*.test.mjs", "**/*.d.ts"]
}
